mod: {
  "${mod}+Return" = "exec alacritty";
  "${mod}+j" = "focus down";
  "${mod}+k" = "focus up";
  "${mod}+h" = "focus left";
  "${mod}+l" = "focus right";
  "${mod}+f" = "fullscreen";
  "${mod}+d" = "exec dmenu_run";
  "${mod}+Shift+r" = "restart";
  "${mod}+apostrophe" = "split horizontal";
  "${mod}+percent" = "split vertical";
  "Mod1+F4" = "kill";

  # ctrl-alt-g
  # jank af: raise_not_tetris doesn't execute
  "Control+Mod1+g" = "exec raise_not_tetris";


  #"${mod}+" = "";
